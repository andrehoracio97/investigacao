<?xml version="1.0"?>
<block>
  <name>Detect Access Code</name>
  <key>cac_cpp_cac_bb</key>
  <category>[cac_cpp]</category>
  <import>import cac_cpp</import>
  <make>cac_cpp.cac_bb($access_code, $threshold)</make>

  <param>
    <name>Access Code</name>
    <key>access_code</key>
    <value>101010</value>
    <type>string</type>
  </param>

  <param>
    <name>Threshold</name>
    <key>threshold</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
