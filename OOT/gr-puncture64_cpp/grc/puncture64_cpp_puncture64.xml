<?xml version="1.0"?>
<block>
  <name>Puncture 64Bits</name>
  <key>puncture64_cpp_puncture64</key>
  <category>[puncture64_cpp]</category>
  <import>import puncture64_cpp</import>
  <make>puncture64_cpp.puncture64($puncsize, $puncpat)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Puncture Size</name>
    <key>puncsize</key>
    <type>int</type>
  </param>

  <param>
    <name>Puncture Pattern</name>
    <key>puncpat</key>
    <type>int</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
