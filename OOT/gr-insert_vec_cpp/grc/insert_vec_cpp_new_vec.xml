<?xml version="1.0"?>
<block>
  <name>new_vec</name>
  <key>insert_vec_cpp_new_vec</key>
  <category>[insert_vec_cpp]</category>
  <import>import insert_vec_cpp</import>
  <make>insert_vec_cpp.new_vec($vec)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Vector</name>
    <key>vec</key>
    <type>int_vector</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
